# vim: syntax=sh

# Aliases for trizen
alias ts='trizen -S'
alias tss='trizen -Ss'
alias tsu='trizen -Syu'
alias tr='trizen -R'
alias trs='trizen -Rs'

# View pdfs with sepia background color
alias mu='mupdf -C DFD0C9'

# Environment variables

export PATH=$PATH:$HOME/apps/bin:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools:$CONFIGS_HOME/bin:$CONFIGS_HOME/scripts